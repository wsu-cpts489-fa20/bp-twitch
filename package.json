{
  "name": "TCGod",
  "version": "1.0.0",
  "description": "Twitch Chat God",
  "main": "./electron/electron.js",
  "scripts": {
    "start": "electron .",
    "build": "babel server.js --out-file server.compiled.js",
    "dev": "nodemon --exec node server.compiled.js",
    "test": "testcafe \"electron:\" \"./tests/**.js\"",
    "ebuild": "npm run build && electron-builder build",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "C Hundhausen",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.10.9",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-twitch-new": "0.0.2",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.13.7",
    "testcafe-browser-provider-electron": "^0.0.15",
    "twitch-auth": "^4.3.6",
    "wait-on": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.1",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "nodemon": "^2.0.5"
  },
  "build": {
    "files": [
      "client/build/**/*",
      "client/node_modules/**/*",
      "electron/**/*",
      "node_modules/**/*",
      "server.compiled.js"
    ]
  }
}
