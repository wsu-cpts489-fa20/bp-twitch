{
  "name": "code-ch18-eb",
  "version": "1.0.0",
  "description": "Testing instructions for deployment to EB",
  "main": "./electron/electron.js",
  "scripts": {
    "start": "node server.compiled.js",
    "edev": "concurrently \"node server.compiled.js\" \"electron .\"",
    "build": "babel server.js --out-file server.compiled.js && cp ./electron/* ./client/build",
    "dev": "nodemon --exec node server.compiled.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ebuild": "npm run build && electron-builder build",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "C Hundhausen",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.10.9",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.13.7",
    "wait-on": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.1",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "nodemon": "^2.0.5"
  }
}
